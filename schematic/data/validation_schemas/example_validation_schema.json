{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example_validation_schema",
    "title": "Example Schema",
    "type": "object",
    "properties": {
        "Clinical Procedure": {
            "enum": [
                "ClinicalTrial",
                "ClinicalIntervention"
            ]
        },
        "Genomic Unit": {
            "enum": [
                "Genotype",
                "Haplotype",
                "SequenceVariant",
                "Genome",
                "Exon",
                "CodingSequence"
            ]
        }
    },
    "required": [
        "Clinical Procedure",
        "Genomic Unit"
    ]
}